text = "*clears throat*"

[sub_resource type="Resource" id="Resource_36tet"]
script = ExtResource("1_xbg0h")
speaker = "Snorlax"
text = "Hi traveller! I'm sorry for the confusion."

[sub_resource type="Resource" id="Resource_11hgt"]
script = ExtResource("1_xbg0h")
speaker = "Snorlax"
text = "I was set to Japanese mode because I came from Hokkaido."

[sub_resource type="Resource" id="Resource_mc74s"]
script = ExtResource("1_xbg0h")
speaker = "Cb"
text = "Oh. Its cool that you can switch languages like that."

[sub_resource type="Resource" id="Resource_tgstr"]
script = ExtResource("1_xbg0h")
speaker = "Snorlax"
text = "Why thank you!"

[sub_resource type="Resource" id="Resource_ttpga"]
script = ExtResource("1_xbg0h")
speaker = "Snorlax"
text = "And it's a good thing you woke me up."

[sub_resource type="Resource" id="Resource_7dqr0"]
script = ExtResource("1_xbg0h")
speaker = "Snorlax"
text = "I had a horrible nightmare where I was a stuffed toy for this girl."

[sub_resource type="Resource" id="Resource_optcy"]
script = ExtResource("1_xbg0h")
