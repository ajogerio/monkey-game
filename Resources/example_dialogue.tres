text = "*wakes up*"

[sub_resource type="Resource" id="Resource_lo113"]
script = ExtResource("1_xbg0h")
speaker = "Snorlax"
text = "E? Dare ga yonda no? Nani ga okotteru no?"

[sub_resource type="Resource" id="Resource_8kop4"]
script = ExtResource("1_xbg0h")
speaker = "Cb"
text = "Huh? Snorlie why are you ching chonging me."

[sub_resource type="Resource" id="Resource_uxs3f"]
script = ExtResource("1_xbg0h")
